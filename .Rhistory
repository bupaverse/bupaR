return(traces)
}
summary_statistics <- function(vector) {
s <- summary(vector)
s <- c(s, St.Dev = sd(vector))
s <- c(s, IQR = s[5] - s[2])
names(s) <- c("min","q1","median","mean","q3","max","st_dev","iqr")
s <- as.data.frame(s)
s <- t(s)
row.names(s) <- NULL
return(s)
}
